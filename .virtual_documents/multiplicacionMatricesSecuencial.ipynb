


import random as r


def generate_matrix(n,m):
    mat=[[0]*m]*n
    for i in range(n):
        for j in range(m):
            mat[i][0]=r.randint(1,222)
    return mat


import numpy as np


def generate_matrix_np(n,m):
    return np.random.randint(1,222,size=(n,m))


%%timeit
generate_matrix(300,300)


%%timeit
generate_matrix_np(300,300)


